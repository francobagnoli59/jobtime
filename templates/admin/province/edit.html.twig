{% extends '@EasyAdmin/page/content.html.twig' %} 


{% block page_content %}
  
       {{ form_start(form, {'attr': {'id': form.vars.id}}  ) }}

    <div class="row" id="title_pers_masotech">
    <div class="col">
    <h4> Elenco province </caption>
    </div>
        <aside>
           <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'Salva modifica' }}
           </button>
        </aside>
    </div>
        <div class="form-error">
            {{ form_errors(form) }}
       </div>
    <div class="row">
        <div class="col">
            {{ form_label(form.id) }}
        </div>
        <div class="col" >
            {{ form_label(form.code) }}
        </div>
        <div class="col" >
            {{ form_label(form.name) }}
        </div>
        <div class="col" >
            {{ form_label(form.createdAt) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_widget(form.id) }}
        </div>
        <div class="col" >
            {{ form_widget(form.code) }}
        </div>
        <div class="col" >
            {{ form_widget(form.name) }}
        </div>
        <div class="col" >
            {{ form_widget(form.createdAt) }}
        </div>
    </div> 
    {{ form_end(form) }}
{% endblock %}
